<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.aladdin.dao.UserMapper">
    <select id="findByUsername" resultType="com.aladdin.entity.User">
      SELECT * FROM t_user
      where
      username=#{username}
    </select>
    <select id="findUserById" resultType="com.aladdin.entity.User">
        SELECT * FROM t_user
      where
      id=#{Id}
    </select>
</mapper>